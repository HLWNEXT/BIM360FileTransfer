<Window x:Class="BIM360FileTransfer.Views.OAuthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BIM360FileTransfer.Views" xmlns:viewmodels="clr-namespace:BIM360FileTransfer.ViewModels"
        xmlns:wpf="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        mc:Ignorable="d" 
        Title="Authentication" Height="650" Width="500"
        MinHeight="500" MinWidth="450">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="7*"/>
            <ColumnDefinition Width="788*"/>
        </Grid.ColumnDefinitions>
        <wpf:ChromiumWebBrowser x:Name="webBrowser" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
